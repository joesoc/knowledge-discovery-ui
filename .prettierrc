{
  "singleQuote": true,
  "printWidth": 100,
  "semi": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "importOrder": ["^[./]"],
  "importOrderSeparation": false,
  "importOrderParserPlugins": ["typescript", "jsx", "classProperties", "decorators-legacy"],
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-organize-attributes"
  ]
}
